{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:vue/vue3-essential", "plugin:@typescript-eslint/recommended", "eslint-config-prettier"],
	"overrides": [],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"parser": "@typescript-eslint/parser"
	},
	"plugins": ["vue", "prettier"],
	"rules": {
		"prettier/prettier": ["error", {}, { "usePrettierrc": true }]
	}
}
